{
  "name": "ciricihq/cake-sitemap",
  "description": "A CakePHP Plugin for adding automatic XML and HTML Sitemaps to an app",
  "type": "cakephp-plugin",
  "keywords": [
    "cakephp",
    "sitemap"
  ],
  "license": "MIT",
  "homepage": "https://github.com/ciricihq/CakePHP-Sitemap",
  "authors": [
    {
      "name": "Justin Yost",
      "homepage": "https://github.com/jtyost2",
      "email": "justin@loadsys.com",
      "role": "Developer"
    },
    {
      "name": "Ã’scar Casajuana",
      "homepage": "https://github.com/elboletaire",
      "email": "oscar@cirici.com",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/ciricihq/CakePHP-Sitemap/issues",
    "source": "https://github.com/ciricihq/CakePHP-Sitemap"
  },
  "require": {
    "php": ">=5.6.0",
    "cakephp/cakephp": "~3.1",
    "composer/installers": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8",
    "loadsys/loadsys_codesniffer": "~3.0",
    "satooshi/php-coveralls": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Sitemap\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cake\\Test\\": "vendor/cakephp/cakephp/test",
      "Sitemap\\Test\\": "tests"
    }
  }
}
